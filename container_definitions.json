[
  {
    "name": "frontend",
    "image": "937976438540.dkr.ecr.ap-northeast-1.amazonaws.com/ptodo/frontend:latest",
    "command": ["yarn", "dev"],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "frontend",
        "awslogs-group": "/ecs/ptodo"
      }
    },
    "portMappings": [
      {
        "protocol":"tcp",
        "containerPort":3000
      }
    ]
  },
  {
    "name": "backend",
    "image": "937976438540.dkr.ecr.ap-northeast-1.amazonaws.com/ptodo/backend:latest",
    "command": ["uvicorn", "app.main:app", "--workers", "1", "--reload", "--host", "0.0.0.0", "--port", "8000"],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "backend",
        "awslogs-group": "/ecs/ptodo"
      }
    },
    "portMappings": [
      {
        "protocol":"tcp",
        "containerPort":8000
      }
    ]
  }
]
